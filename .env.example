# API Configuration
VITE_API_BASE_URL=http://localhost:3001/api

# AI Services
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Authentication - Firebase
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef123456

# Authentication - Auth0
VITE_AUTH0_DOMAIN=your-domain.auth0.com
VITE_AUTH0_CLIENT_ID=your_auth0_client_id
VITE_AUTH0_AUDIENCE=your_auth0_audience

# Database Configuration
VITE_DB_TYPE=mongodb
VITE_DATABASE_URL=mongodb://localhost:27017/feedbackchain

# Blockchain & Web3
VITE_WEB3_RPC_URL=https://polygon-rpc.com
VITE_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000
VITE_PRIVATE_KEY=your_private_key_here

# IPFS Configuration
VITE_IPFS_API_URL=https://api.pinata.cloud/pinning/pinJSONToIPFS
VITE_IPFS_API_KEY=your_pinata_api_key
VITE_IPFS_GATEWAY=https://gateway.pinata.cloud/ipfs/

# File Upload
VITE_UPLOAD_URL=/api/upload
VITE_UPLOAD_API_KEY=your_upload_api_key

# WebSocket
VITE_WEBSOCKET_URL=ws://localhost:3001

# Feature Flags
VITE_ENABLE_BLOCKCHAIN=true
VITE_ENABLE_AI_ANALYSIS=true
VITE_ENABLE_WEBSOCKETS=true
VITE_ENABLE_FILE_UPLOAD=true

# Debug
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info